{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#a1634d",
					"name": "Color Part1",
					"slug": "color-part1"
				},
				{
					"color": "#357997",
					"name": "Color Part2",
					"slug": "color-part2"
				},
				{
					"color": "#6c5992",
					"name": "Color Part3",
					"slug": "color-part3"
				},
				{
					"color": "#e48089",
					"name": "Color Part4",
					"slug": "color-part4"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1180px"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(0.31rem, calc(0.18rem + 0.60vw), 0.63rem)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "clamp(0.63rem, calc(0.36rem + 1.19vw), 1.25rem)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "clamp(0.94rem, calc(0.54rem + 1.79vw), 1.88rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(1.25rem, calc(0.71rem + 2.38vw), 2.50rem)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "clamp(1.88rem, calc(1.07rem + 3.57vw), 3.75rem)",
					"slug": "60",
					"name": "5"
				},
				{
					"size": "clamp(2.50rem, calc(1.43rem + 4.76vw), 5.00rem)",
					"slug": "80",
					"name": "6"
				}
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Noto Sans KR",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/NotoSansKR-Regular-Hestia.woff"]
						},
						{
							"fontFamily": "Noto Sans KR",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": ["file:./assets/fonts/NotoSansKR-Medium-Hestia.woff"]
						},
						{
							"fontFamily": "Noto Sans KR",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/NotoSansKR-Bold-Hestia.woff"]
						},
						{
							"fontFamily": "Noto Sans KR",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": ["file:./assets/fonts/NotoSansKR-Black-Hestia.woff"]
						}
					],
					"fontFamily": "\"Noto Sans KR\", sans-serif",
					"name": "Noto Sans KR",
					"slug": "noto-sans-kr"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.75rem",
						"max": "0.875rem"
					},
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.5rem;"
					},
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.5rem"
					},
					"size": "2.5rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"min": "2.25rem",
						"max": "3rem"
					},
					"size": "3rem",
					"slug": "xxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "3.625rem",
					"fontWeight": "900",
					"lineHeight": "1.3"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(2.625rem, calc(2.625rem + ((1vw - 0.48rem) * 8.4135)), 3.25rem)",
					"lineHeight": "1.4"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "700",
					"lineHeight": "1.6"
				}
			}
		},
		"spacing": {
			"blockGap": "2.5rem",
			"padding": {
				"top": "var(--wp--preset--spacing--40)",
				"right": "var(--wp--preset--spacing--30)",
				"bottom": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	}
}
